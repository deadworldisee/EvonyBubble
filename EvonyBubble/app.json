{
  "expo": {
    "name": "Evony Bubble",
    "slug": "EvonyBubble",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main1.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.vipcommunication.evonybubble",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "teamId": "3UU5J5MSH4",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationWhenInUseUsageDescription": "This app uses location services  for bypassing background app limitation to detect if phone is active.",
        "NSLocationAlwaysUsageDescription": "This app uses location services  for bypassing background app limitation to detect if phone is active."
      }
    },
    "android": {
      "package": "com.vipcommunication.evonybubble",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundImage": "./assets/icon2.png",
        "backgroundColor": "#000000"
      },
      "statusBar": {
        "backgroundColor": "#000000",
        "hidden": true,
        "translucent": true
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/icon2.png"
    },
    "extra": {
      "eas": {
        "projectId": "ebf37bfd-5bd3-4b3a-87af-f5e9a3f38e65"
      }
    },
    "owner": "petrus.b13"
  }
